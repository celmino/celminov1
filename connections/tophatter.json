                            {
                                "id": "get_products",
                                "url": "/products.json",
                                "name": "List Products",
                                "queryParameters": [
                                    {
                                        "id": "status",
                                        "name": "status",
                                        "description": "Filter the results by product status. Possible values are: in_stock, sold_out, enabled, disabled, and deleted. If not set, products with any status are returned.",
                                        "fieldType": "select",
                                        "options": [
                                            "in_stock",
                                            "sold_out",
                                            "enabled",
                                            "disabled",
                                            "deleted"
                                        ]
                                    },
                                    {
                                        "id": "category",
                                        "name": "category",
                                        "description": " Filter the results by product category. If not set, products under all categories are returned. Only the first two levels of categorization are accepted. For example, instead of Electronics  Audio  Headphones you can only specify Electronics Audio for this filter.",
                                        "fieldType": "input"
                                    },
                                    {
                                        "id": "sort",
                                        "name": "show_rejected",
                                        "description": "Enable sorting on returned products. Possible values are below",
                                        "fieldType": "select",
                                        "options": [
                                            "created_at_ascending",
                                            "created_at_descending",
                                            "updated_at_ascending",
                                            "updated_at_descending",
                                            "unique_id_ascending",
                                            "unique_id_descending",
                                            "ratings_average_ascending",
                                            "ratings_average_descending"
                                        ]
                                    }
                                ]
                            },
                            {
                                "id": "get_product",
                                "url": "/products/retrieve.json",
                                "name": "Retrieve a Product",
                                "doesNotSupportPaging": true,
                                "queryParameters": [
                                    {
                                        "id": "identifier",
                                        "name": "identifier",
                                        "description": "The unique ID of the product to retrieve.",
                                        "fieldType": "integer",
                                        "required": true
                                    }
                                ]
                            },
                            {
                                "id": "search_product",
                                "url": "/products/search.json",
                                "name": "Search Products",
                                "doesNotSupportPaging": true,
                                "queryParameters": [
                                    {
                                        "id": "query",
