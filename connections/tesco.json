                                "description": "Query field is mandatory",
                                "queryParameters": [
                                    {
                                        "id": "query",
                                        "name": "Query",
                                        "description": "The search term to query by.",
                                        "fieldType": "input"
                                    },
                                    {
                                        "id": "offset",
                                        "name": "Offset",
                                        "description": "For use to add pagination for search results e.g. 10 to start with the 11th result. Default is 0.",
                                        "fieldType": "input"
                                    },
                                    {
                                        "id": "limit",
                                        "name": "Limit",
                                        "description": "The number of results to return. Default is 10.",
                                        "fieldType": "input"
                                    }
                                ]
                            }
                        ]
                    },
                    {
                        "id": "store",
                        "name": "Store Location",
                        "endpoints": [
                            {
                                "id": "get_store_location",
