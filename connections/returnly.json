                                        "options": [
                                            "authorized",
                                            "in_transit",
                                            "delivered",
                                            "pending_refund",
                                            "refunded"
                                        ]
                                    },
                                    {
                                        "id": "min_updated_at",
                                        "name": "min_updated_at",
                                        "description": "Retrieves the records which are updated after the specified date (format: 2017-02-22T20:28:13.000Z)."
                                    },
                                    {
                                        "id": "max_updated_at",
                                        "name": "max_updated_at",
                                        "description": "Retrieves the records which are updated before the specified date (format: 2017-02-22T20:28:13.000Z)."
                                    },
                                    {
                                        "id": "min_refunded_at",
                                        "name": "min_refunded_at",
                                        "description": "Retrieves the records which are refunded after the specified date (format: 2017-02-22T20:28:13.000Z)."
                                    },
                                    {
                                        "id": "max_refunded_at",
                                        "name": "max_refunded_at",
                                        "description": "Retrieves the records which are refunded before the specified date (format: 2017-02-22T20:28:13.000Z)."
                                    }
                                ]
                            },
