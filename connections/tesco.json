                                "name": "Get Store Location",
                                "resourcePath": "results",
                                "doesNotSupportPaging": true,
                                "queryParameters": [
                                    {
                                        "id": "offset",
                                        "name": "offset",
                                        "description": "For use to add pagination for search results.",
                                        "fieldType": "input"
                                    },
                                    {
                                        "id": "limit",
                                        "name": "Limit",
                                        "description": "The number of results to return.",
                                        "fieldType": "input"
                                    },
                                    {
                                        "id": "sort",
                                        "name": "Sort",
                                        "description": "The search term to query by.",
                                        "fieldType": "input"
                                    },
                                    {
                                        "id": "filter",
                                        "name": "Filter",
                                        "description": "To filter by a single condition use {propertyName}:{propertyValue},To combine conditions that all must be met sepatate by AND - {propertyName1}:{propertyValue1} AND {propertyName2}:{propertyValue2} AND {propertyName3}:{propertyValue3}",
                                        "fieldType": "input"
                                    },
                                    {
                                        "id": "like",
