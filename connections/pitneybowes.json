                                "id": "account_balances",
                                "url": "/shippingservices/v1/developers/:_developerId/merchants/:_postalReportingNumber/account-balance",
                                "name": "Get Account Balances",
                                "description": "This operation retrieves the current balances for the PB Postage Account and, if present, the PB Line of Credit. The operation retrieves the balances at the time the API call was made and does not reflect transactions still in progress.",
                                "doesNotSupportPaging": true,
                                "pathParameters": [
                                    {
                                        "id": "developerId",
                                        "name": "Developer ID",
                                        "fieldType": "input",
                                        "description": "Your Pitney Bowes developer ID."
                                    },
                                    {
                                        "id": "postalReportingNumber",
                                        "name": "Postal Reporting Number",
                                        "fieldType": "input",
                                        "description": "The unique ID used to identify the merchant."
                                    }
                                ]
                            },
                            {
                                "id": "refill_settings",
                                "url": "/shippingservices/v1/developers/:_developerId/merchants/:_postalReportingNumber/autorefillrule",
                                "name": "Get Refill Settings",
                                "description": "This operation retrieves the auto refill settings for a merchant’s PB Postage Account. If auto refill is enabled (which is the default), the postage account automatically replenishes by a pre-determined amount when the balance falls below the pre-determined refill threshold.",
                                "doesNotSupportPaging": true,
                                "pathParameters": [
                                    {
                                        "id": "developerId",
                                        "name": "Developer ID",
                                        "fieldType": "input",
                                        "description": "Your Pitney Bowes developer ID."
                                    },
                                    {
                                        "id": "postalReportingNumber",
                                        "name": "Postal Reporting Number",
                                        "fieldType": "input",
                                        "description": "The unique ID used to identify the merchant."
                                    }
                                ]
                            }
                        ]
                    },
