                                        "fieldType": "input"
                                    },
                                    {
                                        "id": "tpnc",
                                        "name": "tpnc",
                                        "description": "Tesco Product Number - Consumer Unit (UK Identifier) e.g. 255245446",
                                        "fieldType": "input"
                                    },
                                    {
                                        "id": "catid",
                                        "name": "catid",
                                        "description": "Tesco Catalogue Number",
                                        "fieldType": "input"
                                    }
                                ]
                            }
                        ]
                    },
                    {
                        "id": "grocery",
