        "versions": [
            {
                "version": "v1",
                "resources": [
                    {
                        "id": "carrierrules",
                        "name": "Carrier Rules",
                        "endpoints": [
                            {
                                "id": "carrier_rules",
                                "url": "/shippingservices/v1/information/rules/rating-services",
                                "name": "Get Carrier Rules",
                                "description": "This operation retrieves the rules governing the carrier’s services, including the available parcel types and the limits on weights and dimensions.",
                                "doesNotSupportPaging": true,
                                "queryParameters": [
                                    {
                                        "id": "carrier",
                                        "name": "Carrier",
                                        "fieldType": "input",
                                        "description": "The carrier name. Currently this must be set to: USPS",
                                        "required": true
                                    },
                                    {
                                        "id": "originCountryCode",
                                        "name": "Origin Country Code",
                                        "fieldType": "input",
                                        "description": "The two-character ISO country code for the country where the shipment originates.",
                                        "required": true
                                    },
                                    {
                                        "id": "destinationCountryCode",
                                        "name": "Destination Country Code",
                                        "fieldType": "input",
                                        "description": "The two-character ISO country code for the country of the shipment’s destination address.",
                                        "required": true
                                    },
                                    {
                                        "id": "rateTypeId",
                                        "name": "Rate Type Id",
                                        "fieldType": "input",
                                        "description": "The type of rate requested, such as COMMERCIAL_BASE."
                                    },
                                    {
                                        "id": "futureShipmentDate",
                                        "name": "Future Shipment Date",
                                        "fieldType": "input",
                                        "description": "This is required if the shipment is a future day shipment."
                                    },
                                    {
                                        "id": "returnShipment",
                                        "name": "Return Shipment",
                                        "fieldType": "select",
                                        "options": [
                                            "true",
                                            "false"
                                        ],
                                        "description": "If true, provides only services applicable for return shipment."
                                    },
                                    {
                                        "id": "compactResponse",
                                        "name": "Compact Response",
                                        "fieldType": "select",
                                        "options": [
                                            "true",
                                            "false"
                                        ],
                                        "description": "If true, returns only summary, without special service details."
                                    }
                                ]
                            }
                        ]
                    },
                    {
                        "id": "shipments",
                        "name": "Shipments",
                        "endpoints": [
                            {
                                "id": "reprint_ashipment",
                                "url": "/shippingservices/v1/shipments/:_id",
                                "name": "Reprint a Shipment",
                                "description": "This operation reprints a shipment label for which the initial Create Shipment request was successful but for which the label image or URL was lost. To use this API call, you must have the shipmentId from the response to the original request.",
                                "doesNotSupportPaging": true,
                                "pathParameters": [
                                    {
                                        "id": "id",
                                        "name": "Shipment ID",
                                        "fieldType": "input",
                                        "description": " The shipment ID that was issued when shipment label was generated.",
                                        "required": true
                                    }
                                ]
                            }
                        ]
                    },
                    {
                        "id": "manifests",
                        "name": "Manifests",
                        "endpoints": [
                            {
                                "id": "reprint_amanifest",
                                "url": "/shippingservices/v1/manifests/:_id",
                                "name": "Reprint a Manifest",
                                "description": "This operation reprints a manifest for which the initial Create Manifest request was successful but the response object did not save properly.",
                                "doesNotSupportPaging": true,
                                "pathParameters": [
                                    {
                                        "id": "id",
                                        "name": "Manifests ID",
                                        "fieldType": "input",
                                        "description": "The unique ID of the manifest that was assigned when it was created.",
                                        "required": true
                                    }
                                ]
                            }
                        ]
                    },
                    {
                        "id": "tracking",
                        "name": "Tracking",
                        "endpoints": [
                            {
                                "id": "tracking_information",
                                "url": "/shippingservices/v1/tracking/:_number",
                                "name": "Get Package Status",
                                "description": "Shipment labels that are printed using the PB Shipping APIs are automatically tracked. This operation retrieves package status for a label.",
                                "doesNotSupportPaging": true,
                                "pathParameters": [
                                    {
                                        "id": "number",
                                        "name": "Tracking Number",
                                        "fieldType": "input",
                                        "description": "The tracking number for the shipment.",
                                        "required": true
                                    }
                                ],
                                "queryParameters": [
                                    {
                                        "id": "packageIdentifierType",
                                        "name": "Package Identifier Type",
                                        "fieldType": "input",
                                        "description": "Type of package identifier.Valid Value(s): TrackingNumber",
                                        "required": true
                                    },
                                    {
                                        "id": "carrier",
                                        "name": "Carrier",
                                        "fieldType": "input",
                                        "description": "The carrier name. Currently this must be set to: USPS",
                                        "required": true
                                    }
                                ]
                            }
                        ]
                    },
                    {
                        "id": "transactionreports",
                        "name": "Transaction Reports",
                        "endpoints": [
                            {
                                "id": "transaction_reports",
                                "url": "/shippingservices/v4/ledger/developers/:_developerId/transactions/reports",
                                "name": "List Transaction Reports",
                                "description": "This operation retrieves all transactions for a specified period of time.",
                                "resourcePath": "content",
                                "pathParameters": [
                                    {
                                        "id": "developerId",
                                        "name": "Developer ID",
                                        "fieldType": "input",
                                        "description": "Your Pitney Bowes developer ID.",
                                        "required": true
                                    }
                                ],
                                "queryParameters": [
                                    {
                                        "id": "fromDate",
                                        "name": "From Date",
                                        "fieldType": "input",
                                        "description": "The beginning of the date range for transactions.For example:fromDate=2016-10-05T00:00:00Z"
                                    },
                                    {
                                        "id": "toDate",
                                        "name": "To Date",
                                        "fieldType": "input",
                                        "description": "The end of the date range for transactions.For example:toDate=2018-01-02T23:59:00Z"
                                    },
                                    {
                                        "id": "transactionId",
                                        "name": "Transaction ID",
                                        "fieldType": "input",
                                        "description": "The unique string that identifies all the transactions associated with a given shipment."
                                    },
                                    {
                                        "id": "parcelTrackingNumber",
                                        "name": "Parcel Tracking Number",
                                        "fieldType": "input",
                                        "description": "Parcel tracking number of the shipment."
                                    },
                                    {
                                        "id": "merchantId",
                                        "name": "Merchant Id",
                                        "fieldType": "input",
                                        "description": "The value of the postalReportingNumber element in the merchant object. This value is also the merchant’s Shipper ID."
                                    },
                                    {
                                        "id": "transactionType",
                                        "name": "Transaction Type",
                                        "fieldType": "input",
                                        "description": "Transaction Type."
                                    },
                                    {
                                        "id": "printStatus",
                                        "name": "printStatus",
                                        "fieldType": "input",
                                        "description": "The status of a USPS SBR (scan-based return) label."
                                    },
                                    {
                                        "id": "sort",
                                        "name": "Sort",
                                        "fieldType": "select",
                                        "options": [
                                            "asc",
                                            "desc"
                                        ],
                                        "description": "Defines a property to sort on and the sort order."
                                    },
                                    {
                                        "id": "size",
                                        "name": "Size",
                                        "fieldType": "input",
                                        "description": "Page size of the result set for the specified query filters. Specifically, the number of transactions to return per page in the result set."
                                    }
                                ]
                            },
                            {
                                "id": "get_archived_reports",
                                "url": "/shippingservices/v4/ledger/developers/:_developerId/transactions/archived",
                                "name": "Get Archived Reports",
                                "resourcePath": "content",
                                "pathParameters": [
                                    {
                                        "id": "developerId",
                                        "name": "Developer ID",
                                        "fieldType": "input",
                                        "description": "Your Pitney Bowes developer ID.",
                                        "required": true
                                    }
                                ],
                                "queryParameters": [
                                    {
                                        "id": "fromDate",
                                        "name": "From Date",
                                        "fieldType": "input",
                                        "required": true,
                                        "description": "The beginning of the date range for transactions.For example:fromDate=2016-10-05T00:00:00Z"
                                    },
                                    {
                                        "id": "toDate",
                                        "name": "To Date",
                                        "fieldType": "input",
                                        "required": true,
                                        "description": "The end of the date range for transactions.For example:toDate=2018-01-02T23:59:00Z"
                                    },
                                    {
                                        "id": "transactionId",
                                        "name": "Transaction ID",
                                        "fieldType": "input",
                                        "description": "The unique string that identifies all the transactions associated with a given shipment."
                                    },
                                    {
                                        "id": "parcelTrackingNumber",
                                        "name": "Parcel Tracking Number",
                                        "fieldType": "input",
                                        "description": "Parcel tracking number of the shipment."
                                    },
                                    {
                                        "id": "merchantId",
                                        "name": "Merchant Id",
                                        "fieldType": "input",
                                        "description": "The value of the postalReportingNumber element in the merchant object. This value is also the merchant’s Shipper ID."
                                    },
                                    {
                                        "id": "transactionType",
                                        "name": "Transaction Type",
                                        "fieldType": "input",
                                        "description": "Transaction Type."
                                    },
                                    {
                                        "id": "shipDetails",
                                        "name": "shipDetails",
                                        "fieldType": "input",
                                        "description": "If set to 1, the API returns the following fields for each transaction:'dimensionalWeightOz','valueOfGoods','specialServices','customMessage1'.If set to 0, the fields are not returned. "
                                    },
                                    {
                                        "id": "size",
                                        "name": "Size",
                                        "fieldType": "input",
                                        "description": "Page size of the result set for the specified query filters. Specifically, the number of transactions to return per page in the result set."
                                    }
                                ]
                            }
                        ]
