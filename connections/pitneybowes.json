                                "queryParameters": [
                                    {
                                        "id": "sort",
                                        "name": "Sort",
                                        "fieldType": "select",
                                        "options": [
                                            "asc",
                                            "desc"
                                        ],
                                        "description": "Defines a property to sort on and the sort order."
                                    },
                                    {
                                        "id": "size",
                                        "name": "Size",
